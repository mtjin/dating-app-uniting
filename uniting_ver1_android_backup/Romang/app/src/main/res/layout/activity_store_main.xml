                android:adjustViewBounds="true"
                android:src="@drawable/event_dia_android"/>

        </ScrollView>



        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:orientation="horizontal"
            android:clipToPadding="false"
            android:padding="8dp"
            android:weightSum="2">

            <Button
                android:id="@+id/store_btn_close_long"
                android:layout_width="0dp"
                android:layout_height="32dp"
                android:layout_marginEnd="8dp"
                android:background="@drawable/bg_white_button"
                android:text="7일간 그만 보기"
                android:textColor="@color/colorGray80"
                android:textSize="14dp"
                android:textStyle="bold"
                android:layout_weight="1"/>

            <Button
                android:id="@+id/store_btn_close"
                android:layout_width="0dp"
                android:layout_height="32dp"
                android:layout_marginStart="8dp"
                android:background="@drawable/bg_white_button"
                android:text="닫기"
                android:textColor="@color/colorGray80"
                android:textSize="14dp"
                android:textStyle="bold"
                android:layout_weight="1"/>

        </LinearLayout>

    </LinearLayout>


</FrameLayout>